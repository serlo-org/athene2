{% if container is defined %}
    {% if parent is defined %}
        {% set form = navigationForm().getCreatePageForm(container, parent) %}
    {% else %}
        {% set form = navigationForm().getCreatePageForm(container) %}
    {% endif %}
    {% do form.prepare() %}
    {{ form().openTag(form) }}
    {{ formRow(form.get('csrf')) }}
    {% if sendbutton is defined %}
        {{ sendbutton }}
    {% else %}
        {{ formRow(form.get('submit')) }}
    {% endif %}
    {{ form().closeTag() }}
{% endif %}