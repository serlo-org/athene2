{% if uuid is defined %}
    {% set form = uuidForm().getTrashForm(uuid) %}
    {% do form.prepare() %}
    {{ form().openTag(form) }}
    {{ formRow(form.get('csrf')) }}
    {% if sendbutton is defined %}
        {{ sendbutton }}
    {% else %}
        {{ formRow(form.get('submit')) }}
    {% endif %}
{{ form().closeTag() }}
{% endif %}